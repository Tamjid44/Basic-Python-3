#----Dictionaries----
user_info = {
    'Name' : 'Zahin',
    'Age' : 10,
    'Email' : 'abrarzahinxftb@gmail.com'
}
print('Print user name =',user_info['Name'])
print('Print user age =',user_info['Age'])
print('Print user email =',user_info['Email'])

user_info['Address'] = 'Chattogram'
user_info['Age'] = 9
print(user_info)
print(user_info.keys())
print(user_info.values())
print(user_info.items())
del user_info['Email']
print(user_info)
