lst = [1, 5, 2, 9, 3, 22, 13]
def sort_list(lst):
 lst.sort()
 return lst
print('The sorted ascending list is =',sort_list(lst))


#Another way
def sort_list(lst):
    return sorted(lst)

# Given list
numbers = [1, 5, 2, 9, 3, 22, 13]

# Calling the function
sorted_numbers = sort_list(numbers)

# Printing the result
print("Sorted List:", sorted_numbers)
